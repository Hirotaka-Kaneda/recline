{
  "name": "recline",
  "version": "0.0.1",
  "publisher": "julesmons",
  "license": "MPL-2.0",
  "displayName": "Recline",
  "description": "The AI assistant that seamlessly integrates with VSCode to autonomously create, edit, and run terminal commands; redefining how you code.",
  "author": {
    "name": "Jules Mons",
    "email": "jules@jupdev.nl",
    "url": "https://www.julesmons.nl/"
  },
  "engines": {
    "vscode": "^1.91.0"
  },
  "funding": "https://github.com/sponsors/julesmons",
  "homepage": "https://github.com/julesmons/recline#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julesmons/recline.git"
  },
  "bugs": "https://github.com/julesmons/recline/issues",
  "keywords": ["recline", "AI", "agent", "autonomous", "in-IDE"],
  "categories": ["Other"],
  "main": "dist/extension.js",
  "icon": "dist/assets/icons/recline.png",
  "galleryBanner": {
    "color": "#3B82F6",
    "theme": "dark"
  },
  "files": ["LICENSE", "dist"],
  "scripts": {
    "vscode:prepublish": "pnpm run build",
    "build": "rsbuild build",
    "dev": "rsbuild dev",
    "preview": "rsbuild preview",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "package": "vsce package --no-dependencies"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.0",
    "@ai-sdk/openai": "^1.1.0",
    "@ai-sdk/react": "^1.1.0",
    "@vscode-elements/elements": "^1.10.0",
    "ai": "^4.1.0",
    "preact": "^10.25.4",
    "zod": "^3.24.1",
    "@vscode/codicons": "^0.0.36"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rsbuild/core": "^1.1.8",
    "@rsbuild/plugin-preact": "^1.2.0",
    "@types/node": "^22.10.7",
    "@types/vscode": "^1.91.0",
    "typescript": "^5.7.3",
    "repomix": "^0.2.22",
    "@vscode/vsce": "^3.2.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "core-js",
      "@vscode/vsce-sign",
      "keytar"
    ]
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "recline.show",
        "title": "Show",
        "category": "Recline"
      }
    ]
  }
}
